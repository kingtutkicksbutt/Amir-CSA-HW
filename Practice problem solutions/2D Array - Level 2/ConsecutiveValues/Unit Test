public static boolean consecutive(int[][] arr)
{
    for (int i = 0; i < arr.length; i++) {
        for (int j = 0; j < arr[i].length; j++) {
            
            if (i == 2 && j == 2) {
                break;
            }
            
            if (j == 2) {
                if (arr[i][j] == arr[i + 1] [0]) {
                    return true;
                }
                continue;
            }
            
            if (arr[i][j] == arr[i] [j + 1]) {
                return true;
            }
        }
    }
    return false;
}